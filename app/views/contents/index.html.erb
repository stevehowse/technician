<h1>Listing contents</h1>

<table>
  <tr>
    <th>User</th>
    <th>Weekbg</th>
    <th>Week</th>
    <th>Day</th>
    <th>Period</th>
    <th>Room</th>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @contents.each do |content| %>
  <tr>
    <td><%= content.user_id %></td>
    <td><%= content.weekbg %></td>
    <td><%= content.week %></td>
    <td><%= content.day %></td>
    <td><%= content.period %></td>
    <td><%= content.room_id %></td>
    <td><%= content.content %></td>
    <td><%= link_to 'Show', content %></td>
    <td><%= link_to 'Edit', edit_content_path(content) %></td>
    <td><%= link_to 'Destroy', content, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Content', new_content_path %>
